<h1>Editing patient_note</h1>

<% form_for(@patient_note) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :patient %><br />
    <%= f.text_field :patient %>
  </p>
  <p>
    <%= f.label :note_date %><br />
    <%= f.datetime_select :note_date %>
  </p>
  <p>
    <%= f.label :body %><br />
    <%= f.text_area :body %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @patient_note %> |
<%= link_to 'Back', patient_notes_path %>