<h1>Editing patient_alerts</h1>

<% form_for(@patient_alerts) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :patient %><br />
    <%= f.text_field :patient %>
  </p>
  <p>
    <%= f.label :alert_date %><br />
    <%= f.datetime_select :alert_date %>
  </p>
  <p>
    <%= f.label :notes %><br />
    <%= f.text_area :notes %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @patient_alerts %> |
<%= link_to 'Back', patient_alerts_path %>